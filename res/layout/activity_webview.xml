<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="com.sunshireshuttle.driver.activity.LoginActivity">

    <include
        android:id="@+id/cus_toolbar"
        layout="@layout/layout_support_toolbar" />

    <com.sunshireshuttle.driver.widget.ProgressWebView
        android:id="@+id/webView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/cus_toolbar" />
</RelativeLayout>